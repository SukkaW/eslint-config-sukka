{
  "name": "@eslint-sukka/react",
  "version": "6.15.2",
  "description": "Sukka's ESLint config",
  "homepage": "https://github.com/SukkaW/eslint-config-sukka",
  "repository": {
    "url": "https://github.com/SukkaW/eslint-config-sukka",
    "directory": "packages/react"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "Sukka <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.30.0",
    "@eslint-sukka/eslint-plugin-react-jsx-a11y": "workspace:*",
    "@eslint-sukka/shared": "workspace:*",
    "@eslint/compat": "^1.2.7",
    "@next/eslint-plugin-next": "^15.2.0",
    "@stylexjs/eslint-plugin": "^0.11.0",
    "@stylexjs/shared": "^0.11.0",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e552027-20250112",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-ssr-friendly": "^1.3.0"
  },
  "devDependencies": {
    "@eslint-sukka/rollup-config": "workspace:*",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "eslint-plugin-react-prefer-function-component": "^3.4.0"
  }
}
