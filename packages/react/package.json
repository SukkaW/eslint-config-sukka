{
  "name": "@eslint-sukka/react",
  "version": "8.6.1",
  "description": "Sukka's ESLint config",
  "homepage": "https://github.com/SukkaW/eslint-config-sukka",
  "repository": {
    "url": "https://github.com/SukkaW/eslint-config-sukka",
    "directory": "packages/react"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "Sukka <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "@eslint-sukka/eslint-plugin-react-jsx-a11y": "workspace:*",
    "@eslint-sukka/eslint-plugin-stylistic": "workspace:*",
    "@eslint-sukka/shared": "workspace:*",
    "@eslint/compat": "^2.0.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@stylexjs/eslint-plugin": "^0.17.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-ssr-friendly": "^1.3.0",
    "foxts": "^5.2.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.9.4",
    "@eslint-sukka/rollup-config": "workspace:*",
    "eslint-plugin-react-prefer-function-component": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.5.0"
  }
}
