{
  "name": "@eslint-sukka/react",
  "version": "6.2.2",
  "description": "Sukka's ESLint config",
  "homepage": "https://github.com/SukkaW/eslint-config-sukka",
  "repository": {
    "url": "https://github.com/SukkaW/eslint-config-sukka",
    "directory": "packages/react"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin swc3"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "Sukka <https://skk.moe>",
  "license": "MIT",
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.12.4",
    "@eslint-sukka/eslint-plugin-react-jsx-a11y": "workspace:*",
    "@eslint-sukka/shared": "workspace:*",
    "@eslint/compat": "^1.1.1",
    "@next/eslint-plugin-next": "15.0.0-canary.47",
    "@stylexjs/eslint-plugin": "^0.7.5",
    "@stylexjs/shared": "^0.7.5",
    "@stylistic/eslint-plugin-jsx": "^2.7.2",
    "eslint-plugin-react-compiler": "0.0.0-experimental-b8a7b48-20240830",
    "eslint-plugin-react-hooks": "5.1.0-beta-4508873393-20240430",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-ssr-friendly": "^1.3.0",
    "globals": "^15.9.0"
  },
  "devDependencies": {
    "@eslint-sukka/rollup-config": "workspace:*",
    "eslint-plugin-react-prefer-function-component": "^3.3.0"
  }
}
