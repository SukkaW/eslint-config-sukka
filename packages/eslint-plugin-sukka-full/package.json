{
  "name": "@eslint-sukka/eslint-plugin-sukka-full",
  "version": "8.0.2",
  "homepage": "https://github.com/SukkaW/eslint-config-sukka",
  "repository": {
    "url": "https://github.com/SukkaW/eslint-config-sukka",
    "directory": "packages/eslint-plugin-sukka-full"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "mocha --require @swc-node/register src/rules/**/*.test.ts",
    "build": "rollup -c rollup.config.ts --configPlugin swc3"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-sukka": "workspace:*"
  },
  "devDependencies": {
    "@eslint-sukka/rollup-config": "workspace:*",
    "@masknet/eslint-plugin": "^0.4.0",
    "eslint-plugin-unicorn": "^62.0.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
