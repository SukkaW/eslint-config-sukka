    "@eslint-sukka/yaml": "workspace:*",
    "@eslint/core": "^0.16.0",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.13.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.0",
    "@types/stringify-object": "^4.0.5",
    "@typescript-eslint/rule-tester": "^8.46.0",
